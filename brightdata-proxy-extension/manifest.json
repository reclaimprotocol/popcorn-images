{
  "name": "Bright Data Proxy Auth",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Proxy authentication for Bright Data",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestAuthProvider",
    "alarms",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
